<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <properties resource="config.properties"/>
    <!-- mysql驱动目录,自行修改 -->
    <!--<classPathEntry location="D:/maven_local_repository/mysql/mysql-connector-java/5.1.37/mysql-connector-java-5.1.37.jar"/>-->
    <context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <property name="javaFileEncoding" value="UTF-8"/>
        <plugin type="${mapper.plugin}">
            <property name="mappers" value="${mapper.Mapper}"/>
            <property name="caseSensitive" value="true"/>
            <property name="beginningDelimiter" value=""/>
            <property name="endingDelimiter" value=""/>
        </plugin>
        <!--<commentGenerator>-->
        <!--<property name="suppressAllComments" value="true"/>-->
        <!--<property name="suppressDate" value="true"/>-->
        <!--</commentGenerator>-->
        <jdbcConnection driverClass="${jdbc.driverClass}"
                        connectionURL="${jdbc.url}"
                        userId="${jdbc.user}"
                        password="${jdbc.password}">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <javaModelGenerator targetPackage="${entity.targetPacketage}" targetProject="./src/main/java"/>

        <!-- xml文件生成位置,以及包名-->
        <sqlMapGenerator targetPackage="${xml.targetPacketage}"
                         targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <javaClientGenerator targetPackage="${mapper.targetPackage}" targetProject="./src/main/java" type="XMLMAPPER"/>
        <table tableName="user_account" domainObjectName="UserAccount">
            <generatedKey column="id" sqlStatement="JDBC"/>
        </table>
        <table tableName="digital_coin" domainObjectName="DigitalCoin">
        	<generatedKey column="id" sqlStatement="JDBC"/>
        </table>
        <table tableName="consignation_log" domainObjectName="ConsignationLog">
        	<generatedKey column="id" sqlStatement="JDBC"/>
        </table>
        <table tableName="asset_change_log" domainObjectName="AssetChangeLog">
        	<generatedKey column="id" sqlStatement="JDBC"/>
        </table>
        <table tableName="order_log" domainObjectName="OrderLog">
        	<generatedKey column="id" sqlStatement="JDBC"/>
        </table>
        <table tableName="trade_pair_info" domainObjectName="TradePairInfo">
        	<generatedKey column="id" sqlStatement="JDBC"/>
        </table>
        <table tableName="message_transfer_error" domainObjectName="MessageTransferError">
          <generatedKey column="id" sqlStatement="JDBC"/>
        </table>
    </context>
</generatorConfiguration>
